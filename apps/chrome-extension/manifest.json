{
  "manifest_version": 3,
  "name": "WireScript for GitHub",
  "version": "0.0.1",
  "description": "Render WireScript wireframes in GitHub markdown",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}
